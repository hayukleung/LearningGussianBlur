<manifest package="com.hayukleung.learninggussianblur"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:name=".blurkitversion.BlurKitVersionApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".javaversion.JavaVersionDemoActivity">
        </activity>

        <activity android:name=".renderscriptversion.RenderScriptVersionDemoActivity">
        </activity>

        <activity android:name=".blurkitversion.BlurKitVersionDemoActivity">
        </activity>

        <activity android:name=".blurryversion.BlurryVersionDemoActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>
